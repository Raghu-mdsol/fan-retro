<%= form_with url: dashboards_path, scope: "board" do |form| %>
<%= form.label :name %>
<%= form.text_field :name ,class: 'form-control',placeholder: "Name"%><br/>
<%= form.label :description %>
<%= form.text_field :description ,class: 'form-control',placeholder: "Desription" %></br>
<%= form.submit "create" ,:class=>"btn btn-info"%>
<% end %>
<br/>
<% flash.each do |key, value| %>
<div class="alert alert-warning %>"><%= value %></div>
<% end %>


<%= link_to "Back" ,dashboards_path,:class=>"btn btn-info"%>




